//- menu
mixin mnu()
	-
		var nav= {
			"О конференции": "#sRepresent",
			"Программа": "#sProgram",
			"Выставка": "#sExponation",
			"Билеты": "#sTTickets",
			"Контакты": "#sPlace",
			}
	+b.UL.navMenu(itemscope itemtype="http://schema.org/SiteNavigationElement")&attributes(attributes)
		each linkPatch, linkName in nav
			+e.LI.item( itemprop="item" ): +e.A.link(href= linkPatch  itemprop="url")= linkName
//- /menu